a = int (input('Entre com o primeiro valor: '))
b = int (input( 'Entre com o segundo valor: '))
c = int (input('Entre com o terceiro valor: '))

if a > b > c:
    print ('O menor número é: ', c)

elif a > c > b:
    print ('O menor número é: ', b)

elif b > a > c:
    print ('O menor número é: ', c)

elif b > c > a:
    print ('O menor número é: ', a)

elif c > a > b:
    print ('O menor número é: ', b)

elif c > b > a:
    print ('O menor número é: ', a)
